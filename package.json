{
  "name": "vscode-exports-autocomplete",
  "displayName": "exports autocomplete",
  "description": "autocompletes javascript module exports from your project",
  "version": "0.5.2",
  "publisher": "capaj",
  "engines": {
    "vscode": "^1.5.0"
  },
  "galleryBanner": {
    "color": "#f5da55",
    "theme": "dark"
  },
  "categories": ["Other"],
  "keywords": ["javascript", "modules", "imports", "exports", "auto import"],
  "icon": "images/icon.png",
  "activationEvents": ["onLanguage:javascript", "onLanguage:javascriptreact"],
  "main": "./bootstrap",
  "contributes": {
    "commands": [
      {
        "title": "clear and reload exports cache",
        "category": "vscode-exports-autocomplete",
        "command": "vscode-exports-autocomplete.refreshCache"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "vscode-exports-autocomplete",
      "properties": {
        "vscode-exports-autocomplete.cacheSizeLimit": {
          "type": "string",
          "default": "25MB",
          "description":
            "size limit for the cache of parsed exports, shared among all projects"
        },
        "vscode-exports-autocomplete.indentationOverride": {
          "type": "string",
          "default": null,
          "description":
            "generated import statements will always be indented using this string if it is set"
        },
        "vscode-exports-autocomplete.enable": {
          "type": "boolean",
          "default": true,
          "description":
            "it might be useful to disable when you have a project where you can't sacrifice performance"
        },
        "vscode-exports-autocomplete.enableNpmDependencies": {
          "type": "boolean",
          "default": true,
          "description":
            "set this to false when you're annoyed by the stuff you get from your npm modules"
        },
        "vscode-exports-autocomplete.minimumWordLengthToImportOnPaste": {
          "type": "number",
          "default": 3,
          "description":
            "set this to a higher value if 'on paste' is too aggressive for you"
        },
        "vscode-exports-autocomplete.addImportsOnPaste": {
          "type": "boolean",
          "default": true,
          "description":
            "set this to false when you don't want to have imports added on pasting code"
        },
        "vscode-exports-autocomplete.addSpacingAroundCurlyBraces": {
          "type": "boolean",
          "default": false,
          "description":
            "set this to true when you prefer spaces around curly braces"
        },
        "vscode-exports-autocomplete.quoteCharOverride": {
          "type": "string",
          "default": null,
          "enum": ["'", "\"", "`"],
          "description":
            "this allows to override the import style and force the imports to use this string as quotes"
        }
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/capaj/vscode-exports-autocomplete.git"
  },
  "author": "capajj@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/capaj/vscode-exports-autocomplete/issues"
  },
  "homepage": "https://github.com/capaj/vscode-exports-autocomplete#readme",
  "scripts": {
    "postinstall": "node ./node_modules/vscode/bin/install",
    "pretest": "standard && jsinspect ./lib",
    "test": "node ./node_modules/vscode/bin/test && ava"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.43",
    "@types/node": "^8.0.34",
    "ava": "^0.22.0",
    "jsinspect": "^0.12.7",
    "mocha": "^4.0.1",
    "standard": "^10.0.3",
    "typescript": "^2.5.3",
    "vscode": "^1.1.6"
  },
  "ava": {
    "files": ["lib/**/*.spec.js"]
  },
  "dependencies": {
    "babel-traverse": "^6.26.0",
    "babylon": "^6.18.0",
    "babylon-undeclared-identifiers": "^1.0.0",
    "co": "^4.6.0",
    "get-exports-from-file": "^0.5.0",
    "jest-changed-files": "^21.2.0",
    "keep-it-small": "^0.1.0",
    "klaw": "^2.1.0",
    "lodash.camelcase": "^4.3.0",
    "mkdirp": "^0.5.1",
    "mock-require": "^2.0.2",
    "mz": "^2.7.0"
  },
  "standard": {
    "ignore": ["**/fixtures/**"]
  }
}
